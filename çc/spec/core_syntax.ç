#include "../compiler.h"

#if TEST

macro 
{
    assert(IDE_COMPAT == 0);
    return 0;
}

#if IDE_COMPAT
# define __G1__ 0
#endif
macro
{
    if (!strncmp(*src, "__G1__", 6) && !ctx->is.str)
    {
        *src += 6;
        return "41";     
    }
    return 0;
}

#if IDE_COMPAT
# define __G2__ 0
#endif
macro
{
    if (!strncmp(*src, "__G2__", 6) && !ctx->is.str)
    {
        *src += 6;
        return strdup((char[7]) {'_', '_', 'G', '1', '_', '_', 0});     
    }
    return 0;
}

int main()
{
    assert(IDE_COMPAT == 0);
    assert(__G2__ == 41);
    printf("OK\n"); 
}
#endif